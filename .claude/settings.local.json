{
  "permissions": {
    "allow": [
      "Bash(tree:*)",
      "Bash(wc:*)",
      "Bash(node -e:*)",
      "Bash(netstat -ano)",
      "Bash(findstr \":5000\")",
      "Bash(findstr \":4000\")",
      "Bash(findstr \":3000\")",
      "Bash(npm run build:*)"
    ],
    "deny": []
  },
  "enabledMcpjsonServers": [],
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "say 'All done'",
            "timeout": 120
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "say 'Subagent finished'",
            "timeout": 120
          }
        ]
      }
    ],
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/on-notification-say.sh",
            "timeout": 60
          }
        ]
      }
    ]
  }
}
